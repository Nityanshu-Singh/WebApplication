@model List<Question1.Models.Customer>
@{
    ViewBag.Title = "GermanCustomers";
}

<h2>GermanCustomers</h2>

<table>
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <!-- Add more columns as needed -->
    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.CustomerID</td>
            <td>@customer.CompanyName</td>
            <td>@customer.ContactName</td>
            <!-- Add more columns as needed -->
        </tr>
    }
</table>